{
	"compilerOptions": {
        "module": "esnext",
		"target": "es2019",
        "allowJs": true,
		"moduleResolution": "node",
		"strict": true,
		"noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
		"preserveConstEnums":true,
        "sourceMap": true,
        "baseUrl": "src",
        "plugins":[
            {"name":"typescript-styled-plugin"},
            {"name":"typescript-eslint-language-service"}
        ]
	}
}
